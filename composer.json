{
    "name": "a2e/converter",
    "type": "library",
    "description": "Converter ARC2 parsers output to Erfurn format",
    "keywords": ["rdf","sparql"],
    "homepage": "https://github.com/almatyartem/ARC2toErfurtConverter",
    "license": "W3C",
    "authors": [
        {
            "name": "Artem Golodov"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "repositories": [
        {
            "url": "https://github.com/almatyartem/arc2.git",
            "type": "vcs"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "semsol/arc2": "dev-almatyartem",
        "aksw/erfurt": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0"
    },
    "autoload": {
        "classmap": ["./","Models/"],
        "files": ["vendor/aksw/erfurt/library/antlr/Php/util.php"]
    }
}
