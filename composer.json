  "keywords": [
    "rdf",
    "sparql"
  ],
  "homepage": "https://github.com/shedy2/ARC2toErfurtConverter",
  "license": "W3C",
  "authors": [
    {
      "name": "Artem Golodov"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.3.0",
    "wikilect/arc2": "dev-master",
    "aksw/erfurt": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0"
  },
  "autoload": {
    "classmap": [
      "./",
      "Models/"
    ]
  }
